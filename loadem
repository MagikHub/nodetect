local function BWBZVDO_fake_script() -- LoginButton_2.Load 
	local script = Instance.new('LocalScript', LoginButton_2)
    wait(6.71)
	script.Parent.Visible = true
	script.Parent.Parent.KeyEnter.Visible = true
	script.Parent.Parent.GameSupport.Visible = true
	script.Parent.Parent.reorder.Visible = true
	function clicksound()
		spawn(function()
		local s = Instance.new("Sound",workspace)
		s.SoundId = "rbxassetid://421058925"
		s.Volume = 2.5
		s.Playing = true
		wait(0.5)
			s:Destroy()
		end)
	end
	function acceptsound()
		spawn(function()
		local s = Instance.new("Sound",workspace)
		s.SoundId = "rbxassetid://1292194857"
		s.Volume = 2.5
		s.Playing = true
		wait(0.85)
			s:Destroy()
		end)
	end
	function declinesound()
		spawn(function()
		local s = Instance.new("Sound",workspace)
		s.SoundId = "rbxassetid://2755928629"
		s.Volume = 2.5
		s.Playing = true
		wait(0.85)
			s:Destroy()
		end)
	end
	local txt = script.Parent.Parent.KeyEnter
	loads"https://raw.githubusercontent.com/MagikHub/nodetect/master/kyslol"
	function onaccept()
		local tween = game:GetService("TweenService")
		--tween:Create(script.Parent.Parent.reorder,TweenInfo.new(0.2),{Position = UDim2.new(0.943,0,0,0)}):Play()
		wait(1.6)
			script.Parent.Parent.KeyEnter:Destroy()
		script.Parent.Parent.Important.Visible = true
		script.Parent:Destroy()
	end
		
	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Title, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(255,69,72)}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent.Title, TweenInfo.new(0.3), {TextColor3 = Color3.fromRGB(255,255,255)}):Play()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {ImageColor3 = Color3.fromRGB(25,25,25)}):Play()
	end)
	script.Parent.MouseButton1Down:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {ImageColor3 = Color3.fromRGB(173,173,173)}):Play()
	end)
	script.Parent.MouseButton1Up:Connect(function()
		game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {ImageColor3 = Color3.fromRGB(25,25,25)}):Play()
	end)
	local notallowed;
	
	script.Parent.MouseButton1Click:Connect(function()
		for i,d in pairs(_G.Keys) do
			for i,v in pairs(d) do
				if txt.Text == v and string.len(txt.Text) < 61 and txt.Text ~= "" and txt.Text ~= nil and txt.text ~= " " then
					acceptsound()
					spawn(function()
						
						local img=Instance.new'ImageLabel'
						img.BackgroundTransparency=1
						img.BorderSizePixel=0
						img.ZIndex = 7
						img.Image="rbxassetid://2790389767"
						img.ImageColor3=Color3.fromRGB(79, 252, 60)
						img.ScaleType = "Slice"
						img.SliceCenter = Rect.new(8, 8, 248, 248)
						img.SliceScale = 1
						img.Size=UDim2.new(0,0,0,0)
						img.AnchorPoint = Vector2.new(0.5,0.5)
						img.Position=UDim2.new(.5,0,.5,0)
						img.Parent=script.Parent
						img:TweenSize(UDim2.new(1,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,.4)	
						game:GetService("TweenService"):Create(img, TweenInfo.new(0.75), {ImageTransparency = 1}):Play()
						wait(0.8)
						img:Destroy()
					end)
					onaccept()
					notallowed = false
				else
					notallowed = true
				end
			end
		end
		if notallowed == true then
			declinesound()
			spawn(function()
					
					local img=Instance.new'ImageLabel'
					img.BackgroundTransparency=1
					img.BorderSizePixel=0
					img.ZIndex = 7
					img.Image="rbxassetid://2790389767"
					img.ImageColor3=Color3.fromRGB(255,69,72)
					img.ScaleType = "Slice"
					img.SliceCenter = Rect.new(8, 8, 248, 248)
					img.SliceScale = 1
					img.Size=UDim2.new(0,0,0,0)
					img.AnchorPoint = Vector2.new(0.5,0.5)
					img.Position=UDim2.new(.5,0,.5,0)
					img.Parent=script.Parent
					img:TweenSize(UDim2.new(1,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,.4)	
					game:GetService("TweenService"):Create(img, TweenInfo.new(0.75), {ImageTransparency = 1}):Play()
					wait(0.8)
					img:Destroy()
				end)
		end
	end)
	for i,d in pairs(_G.Keys) do
			for i,v in pairs(d) do
				if _G.Settings.key == v and string.len(_G.Settings.key) < 61 and _G.Settings.key ~= "" and _G.Settings.key ~= nil and _G.Settings.key ~= " " then
					acceptsound()
					spawn(function()
						
						local img=Instance.new'ImageLabel'
						img.BackgroundTransparency=1
						img.BorderSizePixel=0
						img.ZIndex = 7
						img.Image="rbxassetid://2790389767"
						img.ImageColor3=Color3.fromRGB(79, 252, 60)
						img.ScaleType = "Slice"
						img.SliceCenter = Rect.new(8, 8, 248, 248)
						img.SliceScale = 1
						img.Size=UDim2.new(0,0,0,0)
						img.AnchorPoint = Vector2.new(0.5,0.5)
						img.Position=UDim2.new(.5,0,.5,0)
						img.Parent=script.Parent
						img:TweenSize(UDim2.new(1,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,.4)	
						game:GetService("TweenService"):Create(img, TweenInfo.new(0.75), {ImageTransparency = 1}):Play()
						wait(0.8)
						img:Destroy()
					end)
					onaccept()
					notallowed = false
				else
					notallowed = true
				end
			end
		end
end
