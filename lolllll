		Out.Name = "Out"
		Out.Parent = Title_11
		Out.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Out.BackgroundTransparency = 1.000
		Out.Position = UDim2.new(0, 0, 0.600000024, 0)
		Out.Size = UDim2.new(0, 117, 0, 8)
		Out.ZIndex = 6
		Out.Font = Enum.Font.GothamBold
		Out.Text = "Output"
		Out.TextColor3 = Color3.fromRGB(255, 255, 255)
		Out.TextSize = 16.000
		Out.TextTransparency = 1.000
		local script = Instance.new('LocalScript', Drop)

		local way = false
		local drop = script.Parent.Parent.Dropped
		script.Parent.MouseButton1Click:Connect(function()
			if way == false then
				way = true
				game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {Rotation = 180}):Play()
				drop:TweenSize(UDim2.new(1,0,0,60),"Out","Linear",0.3,false,nil)
				game:GetService("TweenService"):Create(script.Parent.Parent.Out, TweenInfo.new(0.3), {TextTransparency = 1}):Play()
			else
				way = false
				game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {Rotation = 90}):Play()
				drop:TweenSize(UDim2.new(1,0,0,0),"In","Linear",0.3,false,nil)
				game:GetService("TweenService"):Create(script.Parent.Parent.Out, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
			end
		end)
		
		for i,v in pairs(options) do
			Dropped.CanvasSize = Dropped.CanvasSize + UDim2.new(0, 0, 0, 15)
			if Dropped.Size.Y.Offset ~= 60 and Dropped.Size.Y.Offset ~= 0 then
				Dropped.Size.Y.Offset = Dropped.Size.Y.Offset + 15
			end
			local Thing = Instance.new("TextButton")
			Thing.Name = "Thing"
			Thing.Parent = Dropped
			Thing.Text = v
			Thing.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Thing.BackgroundTransparency = 0.840
			Thing.BorderSizePixel = 0
			Thing.Size = UDim2.new(1, 0, 0, 15)
			Thing.ZIndex = 8
			Thing.Font = Enum.Font.SourceSans
			Thing.TextColor3 = Color3.fromRGB(255, 255, 255)
			Thing.TextSize = 14.000
			Thing.MouseButton1Click:Connect(function()
					way = false
					game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.3), {Rotation = 90}):Play()
					drop:TweenSize(UDim2.new(1,0,0,0),"In","Linear",0.3,false,nil)
					game:GetService("TweenService"):Create(script.Parent.Parent.Out, TweenInfo.new(0.3), {TextTransparency = 0}):Play()
				script.Parent.Parent.Out.Text = Thing.Text
				callback(v)
					wait(0.3)
					
				end)
		end
	end
	return win;
end
return Library;
