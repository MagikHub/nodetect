local Library = {}
function Library:Category(name)
	local Category = Instance.new("TextButton")
	local On_4 = Instance.new("Frame")
	local Category_2 = Instance.new("ScrollingFrame")
	local UIPadding_2 = Instance.new("UIPadding")
	local UIGridLayout_2 = Instance.new("UIGridLayout")
	Category_2.Name = name
	Category_2.Parent = FrameMain
	Category_2.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
	Category_2.BorderSizePixel = 0
	Category_2.ClipsDescendants = false
	Category_2.Selectable = false
	Category_2.Size = UDim2.new(1, 0, 1, 0)
	Category_2.ZIndex = 4
	Category_2.CanvasSize = UDim2.new(0, 0, 0, 0)
	Category_2.ScrollBarThickness = 6
	Category_2.Visible = false
	local Value2 = Instance.new("StringValue")
	
	Category.Name = name
	Category.Parent = List
	Category.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Category.BackgroundTransparency = 1.000
	Category.BorderSizePixel = 0
	Category.Position = UDim2.new(0, 0, 0.0397351012, 0)
	Category.Size = UDim2.new(0, 151, 0, 30)
	Category.ZIndex = 2
	Category.Font = Enum.Font.GothamBold
	Category.Text = " "..name.." "
	Category.TextColor3 = Color3.fromRGB(255, 255, 255)
	Category.TextSize = 16.000
	
	Category.TextXAlignment = Enum.TextXAlignment.Left
	
	Value2.Parent = Category.Parent
	UIPadding_2.Parent = Category_2
	UIPadding_2.PaddingRight = UDim.new(0, 10)
	UIPadding_2.PaddingTop = UDim.new(0, 10)
	
	UIGridLayout_2.Parent = Category_2
	UIGridLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
	UIGridLayout_2.CellPadding = UDim2.new(0, 2, 0, 5)
	UIGridLayout_2.CellSize = UDim2.new(0, 215, 0, 35)
	UIGridLayout_2.FillDirectionMaxCells = 4
	
	On_4.Name = "On"
	On_4.Parent = Category
	On_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	On_4.BorderSizePixel = 0
	On_4.Position = UDim2.new(0.977999985, 0, 0, 0)
	On_4.Size = UDim2.new(0, 2, 1, 0)
	On_4.Visible = false
	On_4.ZIndex = 999
	
	local script = Instance.new('LocalScript', Category)

	script.Parent.MouseButton1Click:Connect(function()
		Category.Parent.Value.Value = script.Parent.Name
		if script.Parent.Parent.Value.Value == script.Parent.Name then
			script.Parent.On.Visible = true
			for i,v in pairs(script.Parent.Parent.Parent.FrameMain:GetChildren()) do
				v.Visible = false
				if v.Name == (script.Parent.Name) then
					v.Visible = true
				end
			end
			for i,v in pairs(script.Parent.Parent:GetChildren())do
				if v ~= script.Parent then
					if v:FindFirstChild("On") ~= nil then
						v:FindFirstChild("On").Visible = false
						script.Parent.On.Visible = true
					end
				end
			end
			for i,v in pairs(script.Parent.Parent.Parent.Bar:GetChildren())do
				if v:IsA("TextButton") then
					if v:FindFirstChild("On") ~= nil then
						v:FindFirstChild("On").Visible = false
						script.Parent.On.Visible = true
					end
				end
			end
		end
	end)
	script.Parent.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(0.3),{BackgroundTransparency = 0.8}):Play()
	end)
	script.Parent.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(0.3),{BackgroundTransparency = 1}):Play()
	end)
	local win = {}
