			script.Parent.Open:TweenSize(UDim2.new(0,225,0,270),"Out","Linear",0.3,false,nil)
			
			tog = true
			game:GetService("TweenService"):Create(script.Parent,TweenInfo.new(0.3),{ImageColor3 = Color3.fromRGB(255,69,72)}):Play()
			clicksound()
