	function win:Slider(name,min,max,callback)
		Category_2.CanvasSize = Category_2.CanvasSize + UDim2.new(0,0,0,(22.5))
		callback = callback or function(x) end
		esti = true
		local Slider = Instance.new("TextLabel")
		local Title_10 = Instance.new("TextLabel")
		local Slid2 = Instance.new("ImageButton")
		local Frame_2 = Instance.new("ImageButton")
		Slider.Name = "Slider"
		Slider.Parent = Category_2
		Slider.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
		Slider.BackgroundTransparency = 1.000
		Slider.BorderSizePixel = 0
		Slider.Position = UDim2.new(0, 0, 0.0139082056, 0)
		Slider.Size = UDim2.new(0, 200, 0, 35)
		Slider.ZIndex = 6
		Slider.Font = Enum.Font.SourceSans
		Slider.Text = " "
		Slider.TextColor3 = Color3.fromRGB(255, 255, 255)
		Slider.TextSize = 14.000
		
		Title_10.Name = "Title"
		Title_10.Parent = Slider
		Title_10.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
		Title_10.BorderSizePixel = 0
		Title_10.ClipsDescendants = true
		Title_10.Position = UDim2.new(0.0500000007, 0, 0, 0)
		Title_10.Size = UDim2.new(1, -10, 0, 35)
		Title_10.ZIndex = 6
		Title_10.Font = Enum.Font.GothamBold
		Title_10.Text = " "..name
		Title_10.TextColor3 = Color3.fromRGB(255, 255, 255)
		Title_10.TextSize = 16.000
		Title_10.TextXAlignment = Enum.TextXAlignment.Left
		Title_10.TextYAlignment = Enum.TextYAlignment.Top
		
		Slid2.Name = "Slid2"
		Slid2.Parent = Title_10
		Slid2.AnchorPoint = Vector2.new(0.5, 0)
		Slid2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Slid2.BackgroundTransparency = 1.000
		Slid2.Position = UDim2.new(0.494441926, 0, 0.492857128, 0)
		Slid2.Size = UDim2.new(0, 190, 0, 16)
		Slid2.ZIndex = 6
		Slid2.Image = "rbxassetid://3570695787"
		Slid2.ImageColor3 = Color3.fromRGB(30, 30, 30)
		Slid2.ScaleType = Enum.ScaleType.Slice
		Slid2.SliceCenter = Rect.new(100, 100, 100, 100)
		Slid2.SliceScale = 0.060
		
		Frame_2.Name = "Frame"
		Frame_2.Parent = Slid2
		Frame_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Frame_2.BackgroundTransparency = 1.000
		Frame_2.Size = UDim2.new(0, 12, 1, 0)
		Frame_2.ZIndex = 7
		Frame_2.Image = "rbxassetid://3570695787"
		Frame_2.ImageColor3 = Color3.fromRGB(255, 0, 0)
		Frame_2.ScaleType = Enum.ScaleType.Slice
		Frame_2.SliceCenter = Rect.new(100, 100, 100, 100)
		Frame_2.SliceScale = 0.060
		
		-----Main Code-----
		
		local realVal = Title_10.Text..": " -- This is going to go to the 1st label
		local Val = Title_10 -- This is going to go to the 1st label
		local Sliderr = Slid2 -- This is going to go to the slide
		local mousepressed = false
		local SliderBtn = Frame_2 -- This is going to go to the slider button
		local Player = game:GetService("Players").LocalPlayer
		local UIS = game:GetService("UserInputService")
		local RuS = game:GetService("RunService")
		local estimate = esti -- changeable (If making a ui library, make these properties of your function instead of variables) also, i have this defaulted to true because it is very useful...
		-- Properties
		local held = false
		local step = 0 -- do not change!!
		local percentage = 0
		function snap(number, factor)
			if factor == 0 then
				return number
			else
				return math.floor(number/factor+0.5)*factor
			end
		end
		SliderBtn.MouseButton1Down:Connect(function()
			mousepressed = true
			held = true
		end)
		SliderBtn.MouseButton1Up:Connect(function()
			mousepressed = false
			held = false
		end)
		SliderBtn.Parent.MouseLeave:connect(function()
		    mousepressed = false
			held = false
		end)
		SliderBtn.Parent.MouseButton1Down:Connect(function()
			mousepressed = true
			held = true
		end)
		SliderBtn.Parent.MouseButton1Up:Connect(function()
			mousepressed = false
			held = false
		end)
		
		RuS.RenderStepped:connect(function(delta)
		if held then
		local MousePos = UIS:GetMouseLocation().X
		local BtnPos = SliderBtn.Position
		local SliderSize = Sliderr.AbsoluteSize.X
		local SliderPos = Sliderr.AbsolutePosition.X
		local pos = snap((MousePos-SliderPos)/SliderSize,step)
		percentage = math.clamp(pos,0,1)
		SliderBtn.Position = UDim2.new(percentage,-(SliderBtn.Size.X.Offset*0.5),(BtnPos.Y.Scale), BtnPos.Y.Offset)
		local bry = (math.floor((0 + (max - 0) * percentage) * max) / max)
		if estimate == true then
		
		local big = math.floor(bry)
		Val.Text = realVal..big..""
		callback(big)
		if min >= big then
		Val.Text = realVal..min
		callback(min)
		end
		elseif estimate == false then
		Val.Text = realVal..bry
		callback(bry)
		if Val.Text <= min then
		Val.Text = realVal..min
		callback(min)
		end
		end
		end
		end)
	end
	function win:Dropdown(name,options,callback)
		Category_2.CanvasSize = Category_2.CanvasSize + UDim2.new(0,0,0,(22.5))
		options = options or {"NO OPTIONS!","Coded by SlaxxF"}
